from math import sqrt
a =int(input('digite el coeficiente de la variable cuadratica '))
b =int(input('digite el coeficiente de la variable lineal '))
c =int(input('digite el termino independiente  '))
x1=0
x2=0

if((b**2)-4*a*c)< 0:
    print('la solucion: ')

else :
    x1 = (-b+sqrt(b**2-(4*a*c)))/(2*a)
    x2 = (-b-sqrt(b**2-(4*a*c)))/(2*a)

    print(x1)
    print(x2)
    print('las soluciones son : ',x1,x2)

    